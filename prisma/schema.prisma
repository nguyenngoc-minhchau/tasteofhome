generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model about_us {
  id               BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  title            String    @db.VarChar(255)
  brief            String?   @db.Text
  content          String?   @db.LongText
  keyword          String?   @db.Text
  meta_description String?   @db.Text
  image            String?   @db.VarChar(255)
  re_name          String    @db.VarChar(255)
  isdefault        Int       @default(0) @db.TinyInt
  priority         Int       @default(1)
  isactive         Int       @default(0) @db.TinyInt
  language_id      Int       @default(2) @db.TinyInt
  created_at       DateTime? @db.Timestamp(0)
  updated_at       DateTime? @db.Timestamp(0)
}

model advertisement {
  id          BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  title       String    @db.VarChar(255)
  brief       String?   @db.Text
  url         String?   @db.VarChar(255)
  new_tab     Int?      @default(0) @db.TinyInt
  image       String?   @db.VarChar(255)
  group       BigInt?   @db.UnsignedBigInt
  language_id Int       @default(2) @db.TinyInt
  priority    Int       @default(1)
  isactive    Int       @default(0) @db.TinyInt
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
}

model agent_commission {
  id         Int       @id @default(autoincrement()) @db.UnsignedInt
