}

model agents {
  id                Int       @id @default(autoincrement()) @db.UnsignedInt
  code              String    @db.VarChar(20)
  name              String    @db.VarChar(255)
  level_id          Int       @default(0) @db.TinyInt
  commission        Int?      @default(0) @db.TinyInt
